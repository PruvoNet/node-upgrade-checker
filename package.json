{
  "name": "dep-checker",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "cli.js",
  "engines": {
    "node": ">=12 <13"
  },
  "scripts": {
    "clean:dist": "rm -rf dist",
    "prebuild": "npm run lint && npm run clean:dist",
    "build": "node_modules/.bin/tsc -p tsconfig.build.json",
    "compile": "node_modules/.bin/tsc -p tsconfig.json",
    "pretest": "npm run lint",
    "pretest:unit": "npm run lint",
    "pretest:e2e": "npm run lint",
    "test:unit": "node_modules/.bin/jest 'test/.*\\.spec\\.ts'",
    "test:e2e": "node_modules/.bin/jest 'testE2E/.*\\.spec\\.ts'",
    "test": "node_modules/.bin/jest 'test/.*\\.spec\\.ts' 'testE2E/.*\\.spec\\.ts'",
    "clean:cover": "rm -rf coverage",
    "precover": "npm run clean:cover",
    "precover:unit": "npm run clean:cover",
    "precover:e2e": "npm run clean:cover",
    "cover:e2e": "node_modules/.bin/jest --coverage 'testE2E/.*\\.spec\\.ts'",
    "cover:unit": "node_modules/.bin/jest --coverage 'test/.*\\.spec\\.ts'",
    "cover": "node_modules/.bin/jest --coverage 'test/.*\\.spec\\.ts' 'testE2E/.*\\.spec\\.ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts' 'testE2E/**/*.ts'"
  },
  "author": "Regev Brody <regevbr@gmail.com>",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/nodegit": "^0.26.1",
    "@types/tmp": "^0.1.0",
    "@types/yaml": "^1.2.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "nodegit": "^0.26.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.1",
    "tmp": "^0.1.0",
    "typeorm": "^0.2.24",
    "yaml": "^1.8.0"
  }
}

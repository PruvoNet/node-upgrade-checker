{
  "name": "node-upgrade-checker",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nuc": "./dist/cli/index.js"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "scripts": {
    "versionupdate": "node_modules/.bin/ncu && npm install",
    "clean:dist": "rm -rf dist && rm -f .buildcache",
    "prebuild": "npm run lint && npm run clean:dist",
    "build": "node_modules/.bin/tsc -p tsconfig.build.json",
    "precompile": "npm run lint",
    "compile": "node_modules/.bin/tsc -p tsconfig.json",
    "pretest": "npm run lint",
    "pretest:unit": "npm run lint",
    "pretest:e2e": "npm run lint",
    "test:unit": "node_modules/.bin/jest 'test/.*\\.spec\\.ts'",
    "test:e2e": "node_modules/.bin/jest 'testE2E/.*\\.spec\\.ts'",
    "test": "node_modules/.bin/jest 'test/.*\\.spec\\.ts' 'testE2E/.*\\.spec\\.ts'",
    "clean:cover": "rm -rf coverage",
    "precover": "npm run clean:cover",
    "precover:unit": "npm run clean:cover",
    "precover:e2e": "npm run clean:cover",
    "cover:e2e": "node_modules/.bin/jest --coverage 'testE2E/.*\\.spec\\.ts'",
    "cover:unit": "node_modules/.bin/jest --coverage 'test/.*\\.spec\\.ts'",
    "cover": "node_modules/.bin/jest --coverage 'test/.*\\.spec\\.ts' 'testE2E/.*\\.spec\\.ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts' 'testE2E/**/*.ts'"
  },
  "author": "Regev Brody <regevbr@gmail.com>",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.9.1",
    "@types/nodegit": "^0.26.1",
    "@types/tmp": "^0.1.0",
    "@types/yaml": "^1.2.0",
    "jest": "^25.1.0",
    "npm-check-updates": "^4.0.4",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "moment": "^2.24.0",
    "nodegit": "^0.26.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.1",
    "tmp": "^0.1.0",
    "typeorm": "^0.2.24",
    "yaml": "^1.8.2"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!<rootDir>/src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "errorOnDeprecated": true,
    "verbose": false,
    "cacheDirectory": "<rootDir>/.cache"
  }
}

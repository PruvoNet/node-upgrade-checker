{
  "name": "dep-checker",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "npm run lint && ./node_modules/.bin/tsc",
    "test": "npm run lint && npm run mocha",
    "testE2e": "npm run lint && npm run mochaE2e",
    "mocha": "node --preserve-symlinks -r source-map-support/register node_modules/mocha/bin/_mocha lib/test/**/*.spec.ts",
    "mochaE2e": "node --preserve-symlinks -r source-map-support/register node_modules/mocha/bin/_mocha lib/testE2e/**/*.spec.ts",
    "mochaAll": "node --preserve-symlinks -r source-map-support/register node_modules/mocha/bin/_mocha lib/test/**/*.spec.ts lib/testE2e/**/*.spec.ts",
    "cover": "rm -rf coverage && nyc --reporter=lcov --reporter=text-summary npm run mochaAll",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'"
  },
  "author": "Regev Brody <regevbr@gmail.com>",
  "files": [
    "lib/*.d.ts",
    "lib/*.js"
  ],
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.10",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "@types/nodegit": "^0.26.1",
    "@types/tmp": "^0.1.0",
    "@types/yaml": "^1.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "lowdb": "^1.0.0",
    "nodegit": "^0.26.5",
    "reflect-metadata": "^0.1.13",
    "tmp": "^0.1.0",
    "yaml": "^1.8.0"
  }
}

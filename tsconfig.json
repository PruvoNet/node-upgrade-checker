{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "checkJs": false,
    "allowJs": false,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "noImplicitUseStrict": false,
    "preserveSymlinks": true,
    "rootDir": "lib",
    "sourceMap": true,
    "lib": [
      "es2019"
    ]
  },
  "include": [
    "lib/**/*.ts"
  ]
}
